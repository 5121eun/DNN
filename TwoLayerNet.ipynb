   "outputs": [],
   "source": [
    "class TwoLayerNet:\n",
    "    def __init__(self, input_size, hidden_size, output_size):\n",
    "        \n",
    "        w1 = np.random.randn(input_size, hidden_size)\n",
    "        b1 = np.zeros(hidden_size)\n",
    "        w2 = np.random.randn(hidden_size, output_size)\n",
    "        b2 = np.zeros(output_size)\n",
    "        \n",
    "        self.params = [w1, b1, w2, b2]\n",
    "        self.grads = []\n",
    "    \n",
    "    def forward(self, x):\n",
    "        w1, b1, w2, b2 = self.params\n",
    "\n",
    "        l1_out = np.matmul(x, w1) + b1\n",
    "        sig_out = self.sigmoid(l1_out)\n",
    "        l2_out = np.matmul(sig_out, w2) + b2\n",
    "        y = self.softmax(l2_out)\n",
    "        \n",
    "        self.xs = x, l1_out, sig_out, l2_out\n",
    "        self.y = y\n",
    "        \n",
    "        return y\n",
    "        \n",
    "    def backward(self, t):\n",
    "        w1, b1, w2, b2 = self.params\n",
    "        x, l1_out, sig_out, l2_out = self.xs\n",
    "        y = self.y\n",
    "        \n",
    "        soft_dout = (y - t) / y.shape[0]\n",
    "        l2_dout = np.matmul(soft_dout, w2.T) \n",
    "        dw2 = np.matmul(sig_out.T, soft_dout)\n",
    "        db2 = np.sum(soft_dout, axis=0)\n",
    "        \n",
    "        sig_dout = l2_dout * (1 - sig_out) * sig_out\n",
    "        \n",
    "        l1_dout = np.matmul(sig_dout, w1.T)\n",
    "        dw1 = np.matmul(x.T, sig_dout)\n",
    "        db1 = np.sum(sig_dout, axis=0)\n",
    "        \n",
    "        self.grads = dw1, db1, dw2, db2\n",
    "            \n",
    "    def update(self, lr):\n",
    "        for i in range(len(self.params)):\n",
    "            self.params[i] -= self.grads[i] * lr\n",
    "         \n",
    "    def sigmoid(self, x):\n",
    "        return 1 / (1 + np.exp(-x))\n",
    "    \n",
    "    def softmax(self, x):\n",
    "        x = x - x.max(axis=1, keepdims=True)\n",
    "        x = np.exp(x)\n",
    "        return x / x.sum(axis=1, keepdims=True)\n",
    "    \n",
    "    def cross_entropy(self, y, t):\n",
    "        return -np.sum(t * np.log(y + 1e-7)) / y.shape[0]"
