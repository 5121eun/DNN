    "loss_list = []\n",
    "total_loss = 0\n",
    "for j in range(epoch):\n",
    "    idx = np.random.permutation(len(x))\n",
    "    mixed_x = x[idx]\n",
    "    mixed_t = t[idx]\n",
    "    \n",
    "    for i in range(int(len(x)/batch_size)):\n",
    "        batch_x = mixed_x[i * batch_size : (i * batch_size) + batch_size]\n",
    "        batch_t = mixed_t[i * batch_size : (i * batch_size) + batch_size]\n",
    "\n",
    "        y = model.forward(batch_x)\n",
    "        model.backward(batch_t)\n",
    "        model.update(lr)\n",
    "        \n",
    "        loss = model.cross_entropy(y, batch_t)\n",
    "        total_loss += loss\n",
    "        \n",
    "        if (i+1) % 10 == 0:\n",
    "            avg_loss = total_loss / (i + 1)\n",
    "            loss_list.append(avg_loss)\n",
    "            print(f'{j} loss:{avg_loss:.2f}')\n",
    "            total_loss = 0\n",
    "            loss_cnt = 0"
